# Development Environment Variables

# Application
APP_NAME=英会話カフェ API
APP_VERSION=1.0.0
ENVIRONMENT=development
DEBUG=true

# Database
DATABASE_URL=postgresql://postgres:password@postgres:5432/english_cafe_db

# CORS
CORS_ORIGINS=http://localhost:3000,http://frontend:3000

# Security
SECRET_KEY=development-secret-key-minimum-32-characters-long-for-security
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=3600

# Security Features
ENABLE_SECURITY_HEADERS=true
ENABLE_INPUT_VALIDATION=true
ENABLE_RATE_LIMITING=true

# Email (Optional for development)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
FROM_EMAIL=info@english-cafe.com

# External APIs (Optional for development)
YOUTUBE_API_KEY=
GOOGLE_MAPS_API_KEY=
RECAPTCHA_SECRET_KEY=

# SNS (Optional for development)
LINE_OFFICIAL_ID=
FACEBOOK_PAGE_URL=
INSTAGRAM_URL=